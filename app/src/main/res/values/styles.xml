<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CountTextStyle">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="AddToCardTextStyle">
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="ResultPriceTextStyle">
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/purple</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="Page2ColorTextStyle">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/gray13</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
    </style>

    <style name="Page2RatingTextStyle">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/dark_blue</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="Page2HintTextStyle">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/gray11</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="Page2PriceTextStyle">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/dark_blue</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="Page2NameTextStyle">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/dark_blue</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
    </style>

    <style name="roundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="CarouselTextStyle">
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/gray9</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="PriceLatestTextStyle">
        <item name="android:textSize">8sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="PriceFlashSaleTextStyle">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="CategoryTextStyle">
        <item name="android:background">@drawable/bg_tv_latest</item>
        <item name="android:paddingEnd">6dp</item>
        <item name="android:paddingStart">6dp</item>
        <item name="android:textSize">7sp</item>
        <item name="android:textColor">@color/gray18</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="CategoryFlashSaleTextStyle">
        <item name="android:background">@drawable/bg_tv_flash_sale</item>
        <item name="android:paddingEnd">8dp</item>
        <item name="android:paddingStart">8dp</item>
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/gray18</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="SaleTextStyle">
        <item name="android:background">@drawable/bg_tv_sale</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="ProductTitleTextStyle">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="ProductTitleFlashSaleTextStyle">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="SpinnerTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/bg_spinner</item>
        <item name="android:paddingStart">15dp</item>
        <item name="android:paddingEnd">15dp</item>
    </style>

    <style name="MainTitleTextStyle">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/gray19</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="ViewAllTextStyle">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/gray11</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="SpinnerTextStyle">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/gray15</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="SearchTextStyle">
        <item name="android:background">@drawable/bg_search</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:textColor">@color/gray15</item>
        <item name="android:textSize">9sp</item>
    </style>

    <style name="TextInputEditTextStyle">
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">7dp</item>
        <item name="android:paddingTop">7dp</item>
        <item name="android:textColorHint">@color/gray12</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="ButtonStyle">
        <item name="android:background">@drawable/bg_button</item>
        <item name="android:fontFamily">@font/montserrat_bold</item>
        <item name="android:layout_marginEnd">43dp</item>
        <item name="android:layout_marginStart">43dp</item>
        <item name="android:paddingBottom">14dp</item>
        <item name="android:paddingTop">14dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/gray4</item>
    </style>

    <style name="MiniButtonStyle">
        <item name="android:background">@drawable/bg_button_mini</item>
        <item name="android:backgroundTint">@color/purple_500</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:minWidth">50dp</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="NavigationTextStyle">
        <item name="android:fontFamily">@font/montserrat_bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="TitleTextStyle">
        <item name="android:fontFamily">@font/montserrat_bold</item>
        <item name="android:textColor">@color/dark_blue</item>
        <item name="android:textSize">26sp</item>
    </style>

    <style name="HintTextStyle">
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:textColor">@color/gray11</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="LinkTextStyle">
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="MainTextStyle">
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextInputLayoutStyle">
        <item name="android:layout_marginEnd">43dp</item>
        <item name="android:layout_marginStart">43dp</item>
        <item name="boxStrokeWidth">0dp</item>
        <item name="boxStrokeWidthFocused">0dp</item>
        <item name="endIconTint">@color/gray14</item>
    </style>

    <style name="ProfileTextStyle">
        <item name="android:fontFamily">@font/montserrat_bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="ChangePhotoTextStyle">
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:textColor">@color/gray11</item>
        <item name="android:textSize">8sp</item>
    </style>

    <style name="ProfileNameTextStyle">
        <item name="android:fontFamily">@font/montserrat_bold</item>
        <item name="android:textColor">@color/gray17</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="ProfileItemViewTextStyle">
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:textColor">@color/gray19</item>
        <item name="android:textSize">14sp</item>
    </style>

</resources>